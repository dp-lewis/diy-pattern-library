<form action="#" method="post" class="form form_login">
	<label>Email</label>
	<input id="login-email" name="login-email" type="text" placeholder="e.g. me@home.com" />
	<label>Password</label>
	<input id="login-password" name="login-password" type="password" />
	{{#parseJSON '{"element": "button", "text": "Login" }' ~}}
	{{>button ~}}
	{{/parseJSON}}
</form>
